{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
    	"include": [ "js", "js/zinc" ],
    	"exclude": ["js/three.min.js", "js/OBJLoader.js", "js/STLLoader.js"], 
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./docs/"
    }
}
